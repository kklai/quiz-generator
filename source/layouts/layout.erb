<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Quiz Generator" %></title>
    
    <%= stylesheet_link_tag "quiz-generator", "quiz" %>

    <%= javascript_include_tag  "jquery.min.js", "tabletop", "getdata", "quiz" %>
  </head>
  
  <body id="top" class="index <%= page_classes %>" style="background-color: #f1f3f2;">
    <%= yield %>
  </body>
</html>